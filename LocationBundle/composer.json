{
    "name": "che/location-bundle",
    "type": "symfony-bundle",
    "description": "Symfony LocationBundle",
    "keywords": [],
    "homepage": "https://symfony.com",
    "license": "MIT",
    "authors": [
        {
            "name": "Maksim Chenov",
            "email": "digital_m@bk.ru"
        }
    ],
    "require": {
        "php": ">=5.5",
        "symfony/http-kernel": "~2.8|~3.0",
        "symfony/twig-bridge": "~2.8|~3.0"
    },
    "require-dev": {
        "symfony/config": "~2.8|~3.0",
        "symfony/yaml": "~2.8|~3.0",
        "symfony/dependency-injection": "~2.8|~3.0",
        "symfony/framework-bundle": "~2.8|~3.0"
    },
    "autoload": {
        "psr-4": { "LocationBundle\\": "" },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "minimum-stability": "dev",
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
